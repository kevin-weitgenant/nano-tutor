{
  "name": "youtube-learn",
  "displayName": "Youtube learn",
  "version": "0.0.1",
  "description": "Chat with youtube videos using built in AI.",
  "author": "kevin castro weitgenant (kevin.weitgenant@gmail.com)",
  "scripts": {
    "dev": "node scripts/dev-with-fix.js",
    "dev:plain": "pnpm with-env plasmo dev",
    "build": "pnpm with-env plasmo build",
    "package": "pnpm with-env plasmo package",
    "with-env": "dotenv -e .env --",
    "postinstall": "node postinstall/run.js",
    "postbuild": "node postbuild/fix-manifest.js"
  },
  "dependencies": {
    "@huggingface/transformers": "3.2.4",
    "@langchain/core": "^1.0.1",
    "@langchain/textsplitters": "^1.0.0",
    "@plasmohq/messaging": "^0.7.2",
    "@plasmohq/storage": "^1.15.0",
    "idb": "^8.0.3",
    "lucide-react": "^0.546.0",
    "mememo": "^0.1.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.4.2",
    "postcss": "^8.4.32",
    "prettier": "3.2.4",
    "tailwindcss": "^3.3.7",
    "tsx": "^4.20.6",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "sidePanel",
      "storage",
      "tabs",
      "webNavigation"
    ],
    "host_permissions": [
      "https://www.youtube.com/*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    }
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
